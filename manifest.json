{
	"manifest_version": 2,

	"name": "Meanings' History",
	"description": "An extension to keep track of words beings searched on google for meanings.",
	"version": "1.0.0",

	"browser_action": {
		"default_icon": "assets/icons/icon-19.jpg",
		"default_popup": "main.html"
	},
	
	"icons": {
		"48": "assets/icons/icon-48.jpg",
		"128": "assets/icons/icon-128.jpg"
	},
	
	"permissions": [
		"history"
	],
	
	"content_scripts": [
		{
			"matches": ["http://www.google.com/*"],
			"js": ["assets/script.js"]
		}
	]
}